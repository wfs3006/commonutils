<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="layout_fill_fill">
        <item name="android:layout_width">fill_parent</item>
	    <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="layout_wrap_wrap">
        <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="layout_wrap_fill">
        <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="layout_fill_wrap">
        <item name="android:layout_width">fill_parent</item>
	    <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="layout_0_fill">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
	    <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="layout_fill_0">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">fill_parent</item>
	    <item name="android:layout_height">0dp</item>
    </style>
    <style name="layout_0_wrap">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
	    <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="layout_wrap_0">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">0dp</item>
    </style>
	<style name="layout_top" parent="layout_fill_fill">
	    <item name="android:background">#eaeaea</item>
	</style>
    <style name="selectableBackground">
        <item name="android:background">?android:attr/selectableItemBackground</item>
    </style>

    <style name="dividerVertical">
        <item name="android:background">?android:attr/dividerVertical</item>
    </style>

    <style name="dividerHorizontal">
        <item name="android:background">?android:attr/dividerHorizontal</item>
    </style>
</resources>
